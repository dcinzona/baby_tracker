    <fields>
        <fullName>Age_Weeks__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT( FLOOR((BLANKVALUE( Date__c , TODAY()) - Baby__r.Birthdate) / 7)) &amp; &apos; weeks old&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age (Weeks)</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Age_Years__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(
IF(
  MONTH(Date__c)&gt;MONTH( Baby__r.Birthdate),
    YEAR(Date__c)-YEAR( Baby__r.Birthdate),  
    IF(AND(MONTH(Date__c) = MONTH( Baby__r.Birthdate), DAY(Date__c) &gt;= DAY( Baby__r.Birthdate)), 
      YEAR(Date__c)-YEAR( Baby__r.Birthdate), 
      (YEAR(Date__c)-YEAR( Baby__r.Birthdate))-1)
)
) &amp; &apos; years old&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age (Years)</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
