    <fields>
        <fullName>Age_Weeks__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT( FLOOR((BLANKVALUE( Date__c , TODAY()) - Baby__r.Birthdate) / 7)) &amp; &apos; weeks old&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age (Weeks)</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Amount_ML__c</fullName>
        <externalId>false</externalId>
        <label>Amount (ML)</label>
        <precision>4</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Amount_OZ__c</fullName>
        <externalId>false</externalId>
        <formula>Amount_ML__c /30</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Amount (OZ)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Amount__c</fullName>
        <description>Returns Feed Amount or Hours Slept based on record type of the record</description>
        <externalId>false</externalId>
        <formula>CASE( RecordType.DeveloperName , 
&apos;Feed&apos;,  TEXT(Amount_ML__c) &amp; &apos; (ml)&apos;, 
&apos;Sleep&apos;,  IF(!ISBLANK( Sleep_End_Datetime__c ), TEXT(Hours_Slept__c) &amp; &apos; hours&apos;, &apos;In progress...&apos;) , 
&apos;Play&apos;,  IF(!ISBLANK( Sleep_End_Datetime__c ), TEXT(Hours_Slept__c) &amp; &apos; hours&apos;, &apos;In progress...&apos;) , 
&apos;&apos;)</formula>
        <inlineHelpText>Returns Feed Amount or Hours Slept based on record type of the record</inlineHelpText>
        <label>Amount</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
