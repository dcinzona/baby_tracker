    <fields>
        <fullName>Hours_Slept__c</fullName>
        <externalId>false</externalId>
        <formula>ROUND((Sleep_End_Datetime__c - Date_Time__c)*24,2)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
