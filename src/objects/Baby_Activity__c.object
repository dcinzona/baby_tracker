        <fullName>Vitamin_D_IU__c</fullName>
        <description>Calculates the amount of Vitamin D IUs per feed</description>
        <externalId>false</externalId>
        <formula>IF(  ISPICKVAL(Food_Type__c, &apos;Breastmilk&apos;)   , 
 Vitamin_D_Drops__c *  $Setup.CT_Defaults__c.Vitamin_D_per_Drop__c 
,  Amount_ML__c *  $Setup.CT_Defaults__c.Vitamin_D_Per_Formula__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Vitamin D IU</label>
